<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="groupwork.chooseGameController" stylesheets="@../css/chooseGame.css">
    <Label text="Enter Game Number:"/>
    <TextField fx:id="gameNumberTextField"/>
    <Button text="Start Game" onAction="#startGame"/>
</VBox>

